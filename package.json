{
  "name": "alchemix-v2-ui",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "abi": "bash ./scripts/updateAbi.sh",
    "abi:main": "bash ./scripts/mainnetAbi.sh",
    "abi:win": "powershell ./scripts/updateAbi.ps1",
    "makeEnv": "bash ./scripts/makeEnv.sh",
    "build": "cross-env NODE_ENV=production webpack",
    "vercel_debug_build": "echo $DEBUG_MODE; yarn makeEnv && webpack",
    "dev:noabi": "yarn format && webpack serve --port 5005",
    "dev": "yarn abi:main && yarn format && webpack serve --port 5005",
    "dev:win": "yarn abi:win && yarn format && webpack serve --port 5005",
    "dev:testnet": "yarn abi && yarn format && webpack serve --port 5005",
    "format": "yarn run prettier && yarn run lint",
    "lint": "yarn run eslint ./src/ --ext .js,.ts,.svelte",
    "prettier": "npx prettier --write src/**/*.{js,svelte,ts,html} --write public/*.html --no-error-on-unmatched-pattern",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-push": {
    "run": "lint",
    "silent": false
  },
  "devDependencies": {
